{
  "name": "stephan_simonett",
  "description": "A small portfolio page for private use",
  "version": "0.0.1",
  "scripts": {
    "watch:tailwind": "postcss static/tailwind.css -o static/index.css -w",
    "dev": "concurrently \"sapper dev\" \"npm run watch:tailwind\"",
    "build": "sapper build",
    "export": "sapper export",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "now-build": "npm run export"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.2",
    "@tailwindcss/ui": "^0.1.3",
    "compression": "^1.7.1",
    "magic-grid": "^3.2.4",
    "polka": "next",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "sapper": "^0.27.9",
    "svelte": "^3.0.0",
    "svelte-loader": "^2.9.0",
    "tailwindcss": "^1.2.0",
    "webpack": "^4.7.0"
  }
}
